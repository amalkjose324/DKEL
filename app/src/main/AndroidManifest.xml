<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.deepika.keralaelectionlive">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".KeralaElectionLive"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DomainWiseActivity"
            android:label="@string/title_activity_domain_wise"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CandidateInfoActivity"
            android:label="@string/title_activity_candidate_info"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LeadingListActivity"
            android:label="@string/title_activity_leading_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".WonListActivity"
            android:label="@string/title_activity_won_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>